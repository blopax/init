]2;cat scripting/*]1;cat#!/bin/bash

cat /etc/passwd | cut -d : -f 1,3,7 | sed 's/:/ /g'
#! /bin/bash

echo 'apt-get update && apt-get upgrade >> /var/log/update_script.log' > ~/update_script.sh;
chmod 777 ~/update_script.sh;
echo '0 4 * * 1 root sh ~/update_script.sh' >> /etc/crontab

#! /bin/bash

if [ $# != 1 ]
then
	echo "Please put a folder as unique argument"
else
	ls -lShp $1 | grep -v /
fi


#! /bin/bash

if [! -e /etc/crontab]
then
	touch /etc/crontab
fi

touch /etc/crontab_md

echo 'if [! -e /etc/crontab || ! -e /etc/crontab_md]
	mail -s 'crontab or crontab_md doesn\'t exist anymore' root

else
	NEW_CRON=`md5sum /etc/crontab`
	OLD_CRON=`cat /etc/crontab_md`
	if [$OLD_CRON != $NEW_CRON]
		mail -s "Crontab has been modified today" root\
		echo $NEW_CRON > /etc/crontab_md
fi' >~/check_crontab.sh

chmod 777 ~/check_crontab.sh
echo '0 0 * * * ~/check_crontab.sh' >> /etc/crontab

#! /bin/bash

echo "42"
